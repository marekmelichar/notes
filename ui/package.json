{
  "name": "epoznamky-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node scripts/setup-dev.cjs && vite --open",
    "dev": "node scripts/setup-dev.cjs && vite",
    "dev:ci": "node scripts/setup-dev.cjs && vite --host",
    "dev:mock": "node scripts/setup-mock.cjs && vite",
    "build": "node -e \"require('fs').writeFileSync('public/version.txt', require('./package.json').version)\" && tsc && vite build",
    "build:mock": "node scripts/setup-mock.cjs && npm run build",
    "preview": "vite preview",
    "preview:mock": "node scripts/setup-mock.cjs && vite preview",
    "format": "prettier --write \"src/**/*.[jt]s{,x}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.[jt]s{,x}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:all": "npm run test:run && npm run pw:test",
    "pw:test": "playwright test",
    "pw:test:ui": "playwright test --ui",
    "pw:codegen": "playwright codegen",
    "pw:report": "playwright show-report",
    "knip": "knip",
    "msw:init": "npx msw init public --save",
    "api:generate": "orval",
    "api:generate:watch": "orval --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@blocknote/core": "^0.46.1",
    "@blocknote/mantine": "^0.46.1",
    "@blocknote/react": "^0.46.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.1.6",
    "@mui/system": "^5.14.1",
    "@mui/x-date-pickers": "^7.26.0",
    "@reduxjs/toolkit": "^2.0.0",
    "@tanstack/react-query": "^5.90.7",
    "axios": "^1.7.7",
    "dayjs": "^1.11.9",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-http-backend": "^3.0.2",
    "keycloak-js": "^26.2.1",
    "notistack": "^3.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.5.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "knip": "^5.65.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.11.6",
    "orval": "^7.17.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
